theme_Publication()+
ylab("R2")+
xlab("log10(P-value)")+
labs(color = "Ethnic group")+
facet_grid(vars(cau_vec),vars(sample_size))+
scale_color_nejm()
p
png(file = paste0("./LD_clumping_result_p_thres_GA_",i1,".png"),
width = 15, height = 10, res = 300,units = "in")
